        "Tags":[
          {"Key":"Name","Value":"STG-VPC-App-SN-2"},
          {"Key":"Environment","Value":"STG"},
          {"Key":"Region","Value":"us-west-2"},
          {"Key":"Product","Value":"CRM"}
        ]
