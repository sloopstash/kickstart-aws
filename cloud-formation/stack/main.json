        "AvailabilityZone":{"Fn::FindInMap":["VPCSNCONF","AppSN1","AZ"]},
        "Tags":[
          {"Key":"Name","Value":"STG-VPC-App-SN-1"},
          {"Key":"Environment","Value":"STG"},
          {"Key":"Region","Value":"us-west-2"},
          {"Key":"Product","Value":"CRM"}
