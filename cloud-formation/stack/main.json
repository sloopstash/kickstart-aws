      }
    },
    "STGVPCLBSN2":{
      "Type":"AWS::EC2::Subnet",
      "Condition":"CheckENV",
      "DependsOn":["STGVPC"],
      "Properties":{
        "VpcId":{"Ref":"STGVPC"},
        "CidrBlock":{"Fn::FindInMap":["VPCSNCONF","LBSN2","CIDR"]},
        "AvailabilityZone":{"Fn::FindInMap":["VPCSNCONF","LBSN2","AZ"]},
        "Tags":[
          {"Key":"Name","Value":"STG-VPC-LB-SN-2"},
          {"Key":"Environment","Value":"STG"},
